<Blnk2Shot>:
    name: "blnk2"
    BoxLayout:
        padding: 0
        spacing: 0
        background_color: 1, 1, 1, 1
        orientation: 'vertical'
        #<---------
        Page:
            text: "Blank2"
        #<---------
        BoxLayout:
            padding: 20
            id: capturedfund
            AbtLabel:
                padding: 0
                orientation: 'vertical'
                size_hint: 1, 1
                BoxLayout:
                    canvas:
                        RoundedRectangle:
                            source: "pic/fund/4.png"
                            size: self.size
                            pos: self.pos
                            radius: [10, 10, 0, 0]
                BoxLayout:
                    orientation: 'vertical'
                    padding: 5
                    TexFnt:
                        text: "textt"
                        font_size: .08*self.parent.width
                    BoxLayout:
                        orientation: 'horizontal'
                        BoxLayout:
                            orientation: 'vertical'
                            padding: 20, 10
                            halign: 'left'
                            valign: 'bottom'
                            TexFnt:
                                text_size: self.size
                                text : "in: fsvdfb"
                            TexFnt:
                                text_size: self.size
                                text : "am: "
                            TexFnt:
                                text_size: self.size
                                text: "bei: fhnd"
                            TexFnt:
                                text_size: self.size
                                text: "#756"
                        BoxLayout:
                            size_hint: .4,1
                            padding: 10
                            DarkButton:
                                text: 'Found'
                                size_hint: 1, .6
                                pos_hint: {'center_y': .5}
        ContainerLabel:
            size_hint: 1, .2





        #<---------Footer
        FooterBox: